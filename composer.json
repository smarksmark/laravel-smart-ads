{
    "name": "5balloons/laravel-smart-ads",
    "description": "Ad, Banners, Callouts Manager for Laravel",
    "keywords": [
        "5balloons",
        "laravel-smart-ads"
    ],
    "homepage": "https://github.com/5balloons/laravel-smart-ads",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tushar Gugnani",
            "email": "tushar@5balloons.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "livewire/livewire": "^3.0",
        "spatie/laravel-package-tools": "^1.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.6",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Smarksmark\\LaravelSmartAds\\": "src",
            "Smarksmark\\LaravelSmartAds\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smarksmark\\LaravelSmartAds\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smarksmark\\LaravelSmartAds\\LaravelSmartAdsServiceProvider"
            ],
            "aliases": {
                "LaravelSmartAds": "Smarksmark\\LaravelSmartAds\\LaravelSmartAdsFacade"
            }
        }
    }
}
